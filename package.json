    "apsl-react-native-button": "^3.1.1",
    "base-64": "^0.1.0",
    "create-react-context": "^0.3.0",
    "debounce": "^1.2.0",
    "flux-standard-action": "^2.0.4",
    "intl": "^1.2.5",
    "moment": "^2.24.0",
